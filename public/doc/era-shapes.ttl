# imCode: #1.2.1.0.0.1
era-sh:operationalpointsShape sh:property era-sh:imCodeOPT .
era-sh:imCodeOPT
	a sh:PropertyShape ;
    era:affectedProperty era:imCode;
    era:affectedClass era:Track;
    era:affectedClass era:OperationalPoint;
    era:scope "local";
    era:validationAfterXML "false"^^xsd:boolean;
